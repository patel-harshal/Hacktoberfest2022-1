class Solution {
public:
    ListNode *detectCycle(ListNode *head) {
        ListNode*slow=head;
        ListNode*fast=head;
        while(fast != NULL && fast->next != NULL){
            slow=slow->next;
            fast=fast->next->next;
            
            if(fast==slow){
            ListNode*start=head;
            while(start != slow)
                {
                    start = start->next;
                    slow = slow->next;
                }
                return start;
            }
            
        }
        
        return NULL;
        
        
        
    }
};
